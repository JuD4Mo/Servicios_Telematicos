;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	empresa.local. root.empresa.local. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

;Servidor de nombres
@	IN	NS	maestro.empresa.local.
@	IN	NS	esclavo.empresa.local.

maestro	IN	A	192.168.50.3
esclavo	IN	A	192.168.50.4

;Cliente 
cliente	IN	A	192.168.50.2

;Alias
master	IN	CNAME	maestro
slave	IN	CNAME	esclavo
www	IN	CNAME	maestro
